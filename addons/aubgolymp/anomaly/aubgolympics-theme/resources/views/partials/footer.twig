<footer>
    <div class="col-md-10 copyright">
    <p >&copy;{{ 'now'|date('Y') }} AUBG Olympics, All rights reserved.</p></div>
       <div class="col-md-2" id="orange"> <ul class="dropdown">
            {% set medias = entries('social', 'medias').paginate() %}
            {{ medias.render()|raw }}
            {% for media in medias %}
                <li>
                    <a href="{{ media.link }}">{{ media.image.make.fit(30,30)|raw }}</a>
                </li>
            {% endfor %}
        </ul>
       </div>
</footer>